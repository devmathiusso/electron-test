{
  "name": "admin-electron",
  "version": "1.0.5",
  "productName": "PedFarma",
  "description": "An administrator pharmacies dashboard",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always",
    "ship": "build --win -p always",
    "build-mac": "electron-builder build --mac",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=dist/mac",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=dist/win --version-string.CompanyName=PA --version-string.FileDescription=PA --version-string.ProductName=\"PedFarma Admin\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pedfarma/admin-electron.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "electron-updater": "^4.0.6",
    "path": "^0.12.7"
  }
}
